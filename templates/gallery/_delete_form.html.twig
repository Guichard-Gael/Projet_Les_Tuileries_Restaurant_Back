<form method="post" action="{{ path('app_gallery_delete', {'id': picture.picture_id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.picture_id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
